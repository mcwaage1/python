    "### sum of digits in a string\n",
    "\n",
    "\n",
    "def main():\n",
    "    \n",
    "    string = input(\"Write a big number:\")\n",
    "    \n",
    "    number = list(string)\n",
    "    print(number)\n",
    "    number = sum_digits(string)\n",
    "    print(number)\n",
    "\n",
    "def sum_digits(digit):\n",
    "    return sum(int(x) for x in digit if x.isdigit())\n",
    "\n",
    "main()"
   ]
